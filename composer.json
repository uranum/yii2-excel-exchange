{
  "name": "uranum/yii2-excel-exchange",
  "description": "Yii2 extension for exchanging a data from DB to excel and back again for Yii2 Framework.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "yii 2",
    "yii excel",
    "exchange data",
    "export excel",
    "import excel"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Eugeny Emelyanov",
      "email": "eugeny@emelyanov.studio",
      "homepage": "http://emelyanov.studio"
    }
  ],
  "support": {
    "issues": "https://github.com/uranum/yii2-excel-exchange/issues?state=open",
    "source": "https://github.com/uranum/yii2-excel-exchange"
  },
  "require": {
    "yiisoft/yii2": "~2.0",
    "yiisoft/yii2-bootstrap": "*",
    "phpoffice/phpexcel": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "uranum\\yii2-excel-exchange\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "uranum\\yii2-excel-exchange\\tests\\": "tests/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}